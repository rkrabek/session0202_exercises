<%= link_to 'New Animal', new_animal_path %>
<ul> <% @animals.each do |animal| %>
	<li><%= link_to animal.name, animal_path(animal) %> | 
		<%= link_to 'Edit', edit_animal_path(animal) %> | 
		<%= link_to 'Destroy', animal_path(animal),
		method: :delete,
		data: {confirm: 'Are you sure?'} %></li>
	<% end %>
</ul>